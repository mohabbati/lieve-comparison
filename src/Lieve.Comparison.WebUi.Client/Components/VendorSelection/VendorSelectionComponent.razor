@rendermode InteractiveAuto
@attribute [StreamRendering]

@if (Vendors.Any() is false)
{
    @for (int i = 0; i < 2; i++)
    {
        <MudSkeleton Height="60px" Width="30%" />
    }
}
else
{
    @foreach (var item in Vendors)
    {
        <MudCheckBox @bind-Value="@item.IsSelected" Size="Size.Large" Color="Color.Tertiary">@item.DisplayName</MudCheckBox>
    }
}
